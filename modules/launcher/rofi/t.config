rofi.color-enabled   : true
rofi.color-window    : #{{ colors.background.1 }}, #{{ colors.foreground.3 }}, #{{ colors.background.1 }}
rofi.color-normal    : #{{ colors.background.1 }}, #{{ colors.foreground.2 }}, #{{ colors.background.1 }}, #{{ colors.background.4 }}, #{{ colors.accent }}
rofi.font            : {{ fonts.monospace }} 14
rofi.lines           : 5
rofi.padding         : {{ dimens.spacing }}
rofi.bw              : {{ dimens.border }}
rofi.width           : 24
rofi.separator-style : none
rofi.hide-scrollbar  : true
rofi.case-sensitive  : true
